    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Aurorawebsoftware\\AApproval\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/auth": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aurorawebsoftware\\AApproval\\AApprovalServiceProvider"
            ]
        }
